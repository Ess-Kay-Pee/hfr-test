<settings>
    <mirrors>
        <mirror>
            <id>openimaj-maven</id>
            <url>https://maven.openimaj.org/</url>
            <mirrorOf>*</mirrorOf>
        </mirror>
    </mirrors>
    <profiles>
        <profile>
            <id>openimaj-repo</id>
            <repositories>
                <repository>
                    <id>mulesoft-release</id>
                    <name>muleSoft release repository</name>
                    <url>http://repository.mulesoft.org/releases/</url>; ==> replace with HTTPS URL https://repository.mulesoft.org/releases/
                    <layout>default</layout>
                </repository>
            </repositories>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>openimaj-repo</activeProfile>
    </activeProfiles>
    <pluginGroups>
        <pluginGroup>org.mortbay.jetty</pluginGroup>
    </pluginGroups>
    <servers>
        <server>
            <id>maven-default-http-blocker</id>
            <configuration>
                <block>false</block>
            </configuration>
        </server>
    </servers>
</settings>
